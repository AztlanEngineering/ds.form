{
  "name": "@fwrlines/ds",
  "version": "1.0.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/fwrlines/ds"
  },
  "description": "fwrlines Design System",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "scripts": {
    "storybook": "LOCAL=true start-storybook -p 6006 -s ./src/scss/fonts,./src/scss/images",
    "now-build": "mkdir ./public && npm run build-storybook --quiet",
    "build": "echo",
    "build-storybook": "rm -rf ./public/* && build-storybook -c .storybook -o public --quiet -s ./src/scss/fonts,./src/scss/images",
    "copy-css:cjs": "rsync -a --prune-empty-dirs --include '*/' --include '*.scss' --exclude '*' src/ui/ dist/cjs/",
    "copy-css:es": "rsync -a --prune-empty-dirs --include '*/' --include '*.scss' --exclude '*' src/ui/ dist/es/",
    "compile:cjs": "NODE_ENV=cjs babel src/ui --out-dir dist/cjs --ignore **/storie*/*.js",
    "compile:es": "NODE_ENV=es babel src/ui --out-dir dist/es --ignore **/stories/*.js",
    "build:cjs": "npm run compile:cjs && npm run copy-css:cjs",
    "build:es": "npm run compile:es && npm run copy-css:es",
    "compile": "npm run pre-compile && npm run build:cjs && npm run build:es",
    "compile:show": "npm run compile && tree dist",
    "xpre-compile": "rm -rf ./dist",
    "pre-compile": "echo",
    "pub": "npm version patch && npm publish",
    "prepare": "npm run compile",
    "commit": "git add . && git commit -m 'Version bump' && git push",
    "bd": "npm run build&&npm run commit "
  },
  "keywords": [],
  "author": {
    "name": "fwrlines",
    "email": "adrian@fwrlines.com",
    "url": "https://github.com/fwrlines"
  },
  "license": "ISC",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@fwrlines/alphabet-scss": "latest",
    "@fwrlines/utils": "latest",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-http": "^1.5.16",
    "apollo-link-retry": "^2.2.15",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "history": "^4.10.1",
    "react-dom": "^16.12.0",
    "react-router": "^5.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@storybook/addon-actions": "^5.3.7",
    "@storybook/addon-docs": "^5.3.7",
    "@storybook/addon-links": "^5.3.7",
    "@storybook/addon-storyshots": "^5.3.7",
    "@storybook/addons": "^5.3.7",
    "@storybook/react": "^5.3.7",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-react-docgen": "^4.1.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "stylelint": "^13.0.0",
    "stylelint-config-palantir": "^4.0.1",
    "stylelint-scss": "^3.13.0"
  }
}
